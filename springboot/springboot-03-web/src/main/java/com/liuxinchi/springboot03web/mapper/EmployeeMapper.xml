<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuxinchi.springboot03web.mapper.EmployeeMapper">

    <select id="queryEmployeeByEmailPwd" resultType="com.liuxinchi.springboot03web.pojo.Employee">
        select * from company.employee where email=#{email} and password=#{password}
    </select>

</mapper>